# Production Environment Variables
NODE_ENV=production

# API Configuration
VITE_API_URL=https://api.diy-humanoid-configurator.com
VITE_APP_URL=https://diy-humanoid-configurator.com

# Performance Settings
VITE_ENABLE_PWA=true
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_ANALYTICS=true

# Cache Settings
VITE_CACHE_VERSION=1.0.0
VITE_CDN_URL=https://cdn.diy-humanoid-configurator.com

# Build Settings
VITE_SOURCE_MAPS=false
VITE_MINIFY=true
VITE_TREE_SHAKING=true

# Performance Budget
VITE_BUNDLE_SIZE_LIMIT=300
VITE_PERFORMANCE_BUDGET_FCP=1500
VITE_PERFORMANCE_BUDGET_LCP=2500