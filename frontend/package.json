{
  "name": "diy-humanoid-configurator",
  "version": "0.7.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:lighthouse": "npx lighthouse http://localhost:4173 --output json --output html --output-path ./lighthouse-report",
    "serve:admin": "vite --port 5174 --base /admin/",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "build:analyze": "vite build --mode analyze",
    "build:stats": "vite build && npx vite-bundle-analyzer dist/assets",
    "perf:build": "npm run build && npm run build:stats",
    "perf:audit": "npm run preview & sleep 3 && npm run test:lighthouse && kill %1",
    "security:audit": "npm audit --audit-level moderate"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^8.8.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@stripe/react-stripe-js": "^3.9.2",
    "@stripe/stripe-js": "^7.9.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "date-fns": "^4.1.0",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.542.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.0",
    "lighthouse": "^12.0.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.9",
    "terser": "^5.27.0",
    "vite": "^5.2.0",
    "vite-bundle-analyzer": "^0.11.0",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^1.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  }
}
